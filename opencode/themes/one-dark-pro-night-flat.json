{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg": "#16191d",
    "bgPanel": "#1e2227",
    "bgElement": "#282c34",
    "fg": "#abb2bf",
    "fgMuted": "#5c6370",
    "black": "#3f4451",
    "red": "#e05561",
    "green": "#8cc265",
    "yellow": "#d18f52",
    "blue": "#4aa5f0",
    "magenta": "#c162de",
    "cyan": "#42b3c2",
    "white": "#d7dae0",
    "brightBlack": "#4f5666",
    "brightRed": "#ff616e",
    "brightGreen": "#a5e075",
    "brightYellow": "#f0a45d",
    "brightBlue": "#4dc4ff",
    "brightMagenta": "#de73ff",
    "brightCyan": "#4cd1e0",
    "brightWhite": "#e6e6e6",
    "cursor": "#528bff",
    "selection": "#677696"
  },
  "theme": {
    "primary": {
      "dark": "blue",
      "light": "blue"
    },
    "secondary": {
      "dark": "cyan",
      "light": "cyan"
    },
    "accent": {
      "dark": "magenta",
      "light": "magenta"
    },
    "error": {
      "dark": "red",
      "light": "red"
    },
    "warning": {
      "dark": "yellow",
      "light": "yellow"
    },
    "success": {
      "dark": "green",
      "light": "green"
    },
    "info": {
      "dark": "brightBlue",
      "light": "brightBlue"
    },
    "text": {
      "dark": "fg",
      "light": "black"
    },
    "textMuted": {
      "dark": "fgMuted",
      "light": "brightBlack"
    },
    "background": {
      "dark": "bg",
      "light": "brightWhite"
    },
    "backgroundPanel": {
      "dark": "bgPanel",
      "light": "white"
    },
    "backgroundElement": {
      "dark": "bgElement",
      "light": "fg"
    },
    "border": {
      "dark": "black",
      "light": "brightBlack"
    },
    "borderActive": {
      "dark": "brightBlack",
      "light": "black"
    },
    "borderSubtle": {
      "dark": "#2c313a",
      "light": "brightBlack"
    },
    "diffAdded": {
      "dark": "green",
      "light": "green"
    },
    "diffRemoved": {
      "dark": "red",
      "light": "red"
    },
    "diffContext": {
      "dark": "fgMuted",
      "light": "fgMuted"
    },
    "diffHunkHeader": {
      "dark": "brightBlack",
      "light": "brightBlack"
    },
    "diffHighlightAdded": {
      "dark": "brightGreen",
      "light": "brightGreen"
    },
    "diffHighlightRemoved": {
      "dark": "brightRed",
      "light": "brightRed"
    },
    "diffAddedBg": {
      "dark": "#1e3a2f",
      "light": "#d4edda"
    },
    "diffRemovedBg": {
      "dark": "#3a1e1e",
      "light": "#f8d7da"
    },
    "diffContextBg": {
      "dark": "bgPanel",
      "light": "white"
    },
    "diffLineNumber": {
      "dark": "brightBlack",
      "light": "fgMuted"
    },
    "diffAddedLineNumberBg": {
      "dark": "#1e3a2f",
      "light": "#d4edda"
    },
    "diffRemovedLineNumberBg": {
      "dark": "#3a1e1e",
      "light": "#f8d7da"
    },
    "markdownText": {
      "dark": "fg",
      "light": "black"
    },
    "markdownHeading": {
      "dark": "blue",
      "light": "blue"
    },
    "markdownLink": {
      "dark": "brightBlue",
      "light": "blue"
    },
    "markdownLinkText": {
      "dark": "cyan",
      "light": "cyan"
    },
    "markdownCode": {
      "dark": "green",
      "light": "green"
    },
    "markdownBlockQuote": {
      "dark": "fgMuted",
      "light": "fgMuted"
    },
    "markdownEmph": {
      "dark": "yellow",
      "light": "yellow"
    },
    "markdownStrong": {
      "dark": "brightYellow",
      "light": "yellow"
    },
    "markdownHorizontalRule": {
      "dark": "brightBlack",
      "light": "fgMuted"
    },
    "markdownListItem": {
      "dark": "blue",
      "light": "blue"
    },
    "markdownListEnumeration": {
      "dark": "cyan",
      "light": "cyan"
    },
    "markdownImage": {
      "dark": "magenta",
      "light": "magenta"
    },
    "markdownImageText": {
      "dark": "brightMagenta",
      "light": "magenta"
    },
    "markdownCodeBlock": {
      "dark": "fg",
      "light": "black"
    },
    "syntaxComment": {
      "dark": "fgMuted",
      "light": "fgMuted"
    },
    "syntaxKeyword": {
      "dark": "magenta",
      "light": "magenta"
    },
    "syntaxFunction": {
      "dark": "blue",
      "light": "blue"
    },
    "syntaxVariable": {
      "dark": "red",
      "light": "red"
    },
    "syntaxString": {
      "dark": "green",
      "light": "green"
    },
    "syntaxNumber": {
      "dark": "yellow",
      "light": "yellow"
    },
    "syntaxType": {
      "dark": "cyan",
      "light": "cyan"
    },
    "syntaxOperator": {
      "dark": "brightCyan",
      "light": "cyan"
    },
    "syntaxPunctuation": {
      "dark": "fg",
      "light": "black"
    }
  }
}
